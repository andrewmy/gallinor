{
    "name": "andrewmy/gallinor",
    "description": "Gallinor — Gallery Minor — reducing your gallery size while maintaining quality.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "monolog/monolog": "^3.9",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "doctrine/coding-standard": "^14.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
          "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "authors": [
      {
        "name": "Andrejs M-J",
        "email": "andrejs@gmail.com"
      }
    ]
}
